{
    "name": "liquidlight/call-to-actions",
    "description": "Shared Call to Action repository and plugin, to allow display of your CTAs",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "homepage": "https://github.com/liquidlight/call_to_actions",
    "readme": "./README.md",
    "keywords": [
        "TYPO3 CMS",
        "CTAs",
        "Promos"
    ],
    "authors": [
        {
            "name": "Liquid Light",
            "email": "info@liquidlight.co.uk",
            "homepage": "https://www.liquidlight.co.uk/"
        },
        {
            "name": "Mike Street",
            "email": "mike@liquidlight.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/liquidlight/call_to_actions/issues",
        "source": "https://github.com/liquidlight/call_to_actions"
    },
    "require": {
        "typo3/cms-core": "^9.5 || ^10.4 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "LiquidLight\\CallToActions\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "call_to_actions"
        }
    },
    "scripts": {
        "prepare-release": [
            "@extension-create-libs",
            "rm -rf .github",
            "rm .gitignore",
            "rm docker-compose.yml",
            "rm .project.php_cs"
        ]
    }
}
